start() {
    printf 'Running complete-upgrade'
    complete-upgrade
    return "$?"
}

stop() {
    # nop
    return 0
}

restart() {
    stop
    sleep 1
    start
}

case "$1" in
    start|stop|restart)
        "$1";;
    reload)
        restart;;
    *)
        echo "Usage: $0 {start|stop|restart|reload}"
        exit 1
esac